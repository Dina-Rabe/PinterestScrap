{
    "manifest_version": 3,
    "name": "Web pinterest Scraper",
    "version": "1.0",
    "description": "Ceci est un outil Web pinterest Scraper",
    "permissions": ["activeTab", "storage", "scripting", "tabs", "unlimitedStorage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
      }
    },
    "icons": {
      "16": "icon/icon16.png",
      "48": "icon/icon48.png",
      "128": "icon/icon128.png"
    },
    "host_permissions": [
      "https://*.pinterest.*/"
    ]
  }